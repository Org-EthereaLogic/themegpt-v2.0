{
  "hosting": {
    "public": "apps/web/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "theme-gpt-web",
          "region": "us-central1"
        }
      }
    ]
  }
}
